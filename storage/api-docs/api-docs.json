{
    "openapi": "3.0.0",
    "info": {
        "title": "Greenery",
        "description": "Greenery",
        "contact": {
            "email": "natghi2010@gmail.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": ""
        }
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Login"
                ],
                "summary": "Login",
                "description": "Login",
                "operationId": "Login",
                "requestBody": {
                    "description": "Pass user credentials",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user1@mail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "PassWord12345"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful operation"
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/api/history/": {
            "get": {
                "tags": [
                    "History"
                ],
                "summary": "Get List of History",
                "description": "Returns list of History",
                "operationId": "getHistoryList",
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "required": [
                                        "email",
                                        "password"
                                    ],
                                    "properties": {
                                        "email": {
                                            "type": "string",
                                            "format": "email",
                                            "example": "user1@mail.com"
                                        },
                                        "password": {
                                            "type": "string",
                                            "format": "password",
                                            "example": "PassWord12345"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
        },
        "/api/history/{id}": {
            "get": {
                "tags": [
                    "History"
                ],
                "summary": "Get history by ID",
                "description": "Get history by Id",
                "operationId": "gethistory",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "Search for one history",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "default": {
                        "description": "Error"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/report": {
            "post": {
                "tags": [
                    "Report"
                ],
                "summary": "Report",
                "description": "Report",
                "operationId": "Report",
                "requestBody": {
                    "description": "Post Reporting Data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "reporter_id",
                                    "image",
                                    "latitude",
                                    "longitude"
                                ],
                                "properties": {
                                    "reporter_id": {
                                        "type": "integer",
                                        "format": "integer",
                                        "example": "1"
                                    },
                                    "image": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "base64"
                                    },
                                    "latitude": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "9.23243324"
                                    },
                                    "longitude": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "9.77243324"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful operation"
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Greenery",
            "description": "API Endpoints of Greenery App"
        }
    ]
}